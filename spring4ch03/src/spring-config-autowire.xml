<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="foo" class="main.java.autowire.Foo"/>
    <bean id="bar1" class="main.java.autowire.Bar"/>
    <bean id="targetByName" autowire="byName" class="main.java.autowire.Target" lazy-init="true"/>
    <bean id="targetByType" autowire="byType" class="main.java.autowire.Target" lazy-init="true"/>
    <bean id="targetConstructor" autowire="constructor" class="main.java.autowire.Target" lazy-init="true"/>

</beans>