package lesson2;

public class Task3 {

	public static void main(String[] args) {
		double a = 3;
		double b = 5;
		double c = 2;
		solveQuadratic(a, b, c);
	}

	static void solveQuadratic(double a, double b, double c) {

		double x1 = -(b/a)/2 + Math.sqrt(((b/a)/2)*((b/a)/2)-(c/a));
		double x2 = -(b/a)/2 - Math.sqrt(((b/a)/2)*((b/a)/2)-(c/a));

//		if (d < 0)
//			System.out.println("Рівняння не має речових коренів");
//		else
			System.out.println("Корені рівняння: х1= " + x1 + " x2= "+x2);
	}

}
