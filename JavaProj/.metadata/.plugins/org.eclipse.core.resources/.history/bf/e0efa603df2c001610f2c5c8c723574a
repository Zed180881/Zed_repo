package lesson4;

public class PersonalComputer {
	
	Cpu cpu = new Cpu();
	Ram ram = new Ram();
	SSD ssd;
	
	public Cpu getCpu() {
		return cpu;
	}
	public void setCpu(Cpu cpu) {
		this.cpu = cpu;
	}
	public Ram getRam() {
		return ram;
	}
	public void setRam(Ram ram) {
		this.ram = ram;
	}
	public SSD getSsd() {
		return ssd;
	}
	public void setSsd(SSD ssd) {
		this.ssd = ssd;
	}
	@Override
	public String toString() {
		return "PersonalComputer [cpu=" + cpu + ", ram=" + ram + ", ssd=" + ssd
				+ "]";
	}
	
	

}
