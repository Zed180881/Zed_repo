package lesson9Dz;

import java.util.ArrayList;
import java.util.List;

public class Person {
	
	private String name;
	private List<Pet> pets;
	
	public Person(String name, Pet...pets) {		
		this.name = name;
		this.pets = new ArrayList<Pet>();
		for (Pet pet: pets){
			pet.setPetOwner(this);
			this.pets.add(pet);
		}
	}
	public String getName() {
		return name;
	}
	public List<Pet> getPets() {
		return pets;
	}
	
	

}
