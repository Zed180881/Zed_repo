package lesson2;

import java.util.Scanner;

public class Task11 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		System.out
				.println("Введіть натуральне число для визначення дільників: ");
		int n = in.nextInt();
		in.close();
		naturalDiv(n);

	}

	static void naturalDiv(int n) {
		if (n < 1)
			System.out.println("Число повинне бути більше нуля!");
		else {
			System.out.println("Позитивні подільники числа " + n + ":");
			for (int i = n - 1; i >= 1; i--) {
				if (n % i == 0)
					System.out.println(i);
			}
		}
	}
}
