	collection.getObjectArray()[0] = 25;
	collection.getObjectArray()[1] = 17;
	collection.getObjectArray()[2] = 38;
	collection.getObjectArray()[3] = 45;
	collection.getObjectArray()[4] = 16;
	collection.getObjectArray()[5] = 12;
	collection.getObjectArray()[6] = 58;
	collection.getObjectArray()[7] = 69;
	collection.getObjectArray()[8] = 71;
	collection.getObjectArray()[9] = 128;
    }

    @After
    public void clear() {
	collection = new Collection(10);
    }

    @Test
    public void iteratorFirstTest() {
	Collection.FirstIterator iterator = collection.new FirstIterator();
	fail("Not yet implemented");
    }

