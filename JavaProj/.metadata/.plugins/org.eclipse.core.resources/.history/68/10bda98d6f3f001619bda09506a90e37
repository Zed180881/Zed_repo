package lesson10Dz;

import static org.junit.Assert.*;

import java.util.Arrays;

import lesson10Dz.Collection.FirstIterator;

import org.junit.After;
import org.junit.Before;
import org.junit.Test;

public class CollectionTest {

    private static final int TEST_SIZE = 10;
    private Collection collection = new Collection(TEST_SIZE);
    private Object[] outputArray = new Object[TEST_SIZE];

    @Before
    public void init() {
	for (int i = 0; i < collection.getObjectArray().length; i++) {
	    collection.getObjectArray()[i] = (int) Math.random()
		    * Integer.MAX_VALUE;

	}

	collection.getObjectArray()[0] = 25;
	collection.getObjectArray()[1] = 17;
	collection.getObjectArray()[2] = 38;
	collection.getObjectArray()[3] = 45;
	collection.getObjectArray()[4] = 16;
	collection.getObjectArray()[5] = 12;
	collection.getObjectArray()[6] = 58;
	collection.getObjectArray()[7] = 69;
	collection.getObjectArray()[8] = 71;
	collection.getObjectArray()[9] = 128;
    }

    @After
    public void clear() {
	collection = new Collection(10);
    }

    @Test
    public void iteratorFirstTest() {
	Collection.FirstIterator iterator = collection.new FirstIterator();
	while (iterator.hasNext()) {
	    System.out.println();

	}
    }

}
