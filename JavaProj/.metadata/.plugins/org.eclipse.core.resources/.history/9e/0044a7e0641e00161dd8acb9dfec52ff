package lesson2;

public class Task3 {

	public static void main(String[] args) {
		double a = 11.5;
		double b = 13.4;
		double c = 109.6;
		solveQuadratic(a, b, c);
	}

	static void solveQuadratic(double a, double b, double c) {

		double d = b * b - 4 * a * c;

		if (d < 0)
			System.out.println("Рівняння не має речових коренів");

		System.out.println("Корінь рівняння: "
				+ ((-b + Math.sqrt(d)) / (2 * a)));
	}

}
