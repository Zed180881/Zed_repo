package lesson1;

public class Car {

	String brand;
	double fuel;
	int maxSpeed;

	public Car() {
	}

	public Car(String brand, double fuel, int maxSpeed) {
		this.brand = brand;
		this.fuel = fuel;
		this.maxSpeed = maxSpeed;
	}

	public void drive() {
		System.out.println(this.brand+": brum-brum");
	}
}
