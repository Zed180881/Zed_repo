package lesson9Dz;

public class Pet {

	private String name;
	private Person petOwner;

	public Pet(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public Person getPetOwner() {
		return petOwner;
	}

	public void setPetOwner(Person petOwner) {
		this.petOwner = petOwner;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((name == null) ? 0 : name.hashCode());
		result = prime * result
				+ ((petOwner == null) ? 0 : petOwner.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Pet other = (Pet) obj;
		if (name == null) {
			if (other.name != null)
				return false;
		} else if (!name.equals(other.name))
			return false;
		if (petOwner == null) {
			if (other.petOwner != null)
				return false;
		} else if (!petOwner.equals(other.petOwner))
			return false;
		return true;
	}
	
	
	
	

}
